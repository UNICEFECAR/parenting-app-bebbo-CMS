					<div class="cta-block--button center">
						<a href="{{ path('<front>') }}" target="" class="btn primary outlined primary-outlined" data-action="{{ path('<front>') }}" data-bound="click" data-label="Back to our home page">{{ "Back to our home page"|t }}</a>
					</div>
